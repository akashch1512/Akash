{
    "builds": [
        {
            "src": "CrystalSource/wsgi.py",  
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "10mb",  
                "runtime": "python3.9"    
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",  
            "dest": "CrystalSource/wsgi.py"  
        }
    ]
}
